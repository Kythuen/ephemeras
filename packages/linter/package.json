{
  "name": "@ephemeras/linter",
  "version": "1.4.1",
  "description": "Tool to quickly add code lint for your project ",
  "keywords": [
    "ephemeras",
    "command line",
    "code format",
    "commit standardized"
  ],
  "author": {
    "name": "ephemeras",
    "email": "ephemeras@qq.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "files"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./dist/*": "./dist/*"
  },
  "bin": {
    "linter": "dist/index.js"
  },
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "test": "echo \"@ephemeras/linter ignore\"",
    "test:coverage": "echo \"@ephemeras/linter coverage ignore\""
  },
  "dependencies": {
    "@ephemeras/fs": "workspace:*",
    "@ephemeras/profile": "workspace:*",
    "@ephemeras/parser": "workspace:*",
    "cac": "^6.7.14",
    "execa": "4.1.0",
    "gradient-string": "^2.0.2",
    "i18next": "^23.5.1",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.4",
    "@types/prompts": "^2.4.7"
  }
}
